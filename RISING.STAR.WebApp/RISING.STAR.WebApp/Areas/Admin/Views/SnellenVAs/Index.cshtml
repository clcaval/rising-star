@model IEnumerable<RISING.STAR.DAL.SnellenVA>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_LayoutInner.cshtml";
}



<div class="col-sm-9 col-sm-offset-3 col-lg-10 col-lg-offset-2 main">

    <h2>Index</h2>

    <div class="row">
        <div class="col-md-2">
            <p>
                @Html.ActionLink("Create New", "Create")
            </p>
        </div>
        <div class="col-md-2">
            <a href="@Url.Action("AdminDash", "Admin", new { Area = "" })">
                Back to Admin Dashboard
            </a>
        </div>
    </div>
    <table class="table">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.SnellenVA1)
            </th>
            <th></th>
        </tr>

        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.SnellenVA1)
                </td>
                <td>
                    @Html.ActionLink("Edit", "Edit", new { id = item.VAID }) |
                    @Html.ActionLink("Details", "Details", new { id = item.VAID }) |
                    @Html.ActionLink("Delete", "Delete", new { id = item.VAID })
                </td>
            </tr>
        }

    </table>

</div>